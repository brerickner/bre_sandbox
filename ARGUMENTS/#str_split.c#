#include <stdio.h>
#include <string.h>
#include <unistd.h>
#include <stdlib.h>
int print_string(char *str)
{
	return (write(1, str, strlen(str)));
}
void str_split(char **str)
{
	char *tokens;
/*	char **buffer = malloc(strlen(str) * sizeof(char));*/

/*	*buffer = str;*/

	printf("here");
	tokens = strtok(*str, " ");

	while(tokens != NULL)
	{
		printf("%s",tokens);
		tokens = strtok(NULL, " ");
	}
	printf("%s", tokens);
/*	free(tokens);*/

}
int main (void)
{
	char *testStr = "test to see if string splits";
	char **buffer = malloc(1024);
	*buffer = testStr;
	str_split(buffer);
	return (0);
}
